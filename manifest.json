{
  "manifest_version": 3,
  "name": "YouTube Mix Removal",
  "version": "1.0",
  "description": "Get rid of YouTube mixes for a cleaner experience.",
  "permissions": ["webNavigation", "tabs", "storage"],
  "host_permissions": ["*://www.youtube.com/*"],
  "background": {
    "service_worker": "dist/background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "popup/icon16.png",
      "32": "popup/icon32.png",
      "48": "popup/icon48.png",
      "128": "popup/icon128.png"
    }
  },
  "icons": {
    "16": "popup/icon16.png",
    "32": "popup/icon32.png",
    "48": "popup/icon48.png",
    "128": "popup/icon128.png"
  }
}
